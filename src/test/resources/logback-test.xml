<?xml version="1.0" encoding="UTF-8" ?>
<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} %-5level %logger{42} - %msg%n</pattern>
		</encoder>
	</appender>
	
	<logger name="nl.tudelft.ewi" level="INFO"></logger>
	<logger name="liquibase" level="INFO"></logger>
    <logger name="org.eclipse.jetty" level="INFO"></logger>
    <logger name="log4j.logger.org.hibernate.type" level="TRACE"/>


	<root level="WARN">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>
